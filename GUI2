import javax.swing.*;
import BreezySwing.*;
import java.awt.Color;

public class GUI2 extends GBFrame {

	// create panel that has all the functionality
	GBPanel panel = addPanel(new GUI2PANEL(Color.white), 1, 1, 1, 1);

	// main method
	public static void main(String[] args) {
		JFrame frm = new GUI2();
		frm.setSize(550, 600);
		frm.setVisible(true);
		frm.setTitle("Ultimate Tic-Tac-Toe");
	}

}
